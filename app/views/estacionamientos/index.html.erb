<p id="notice"><%= notice %></p>

<h1>Listing Estacionamientos</h1>

<table>
  <thead>
    <tr>
      <th>Fe inicio</th>
      <th>Fe final</th>
      <th>Monto pagar</th>
      <th>Mensaje</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estacionamientos.each do |estacionamiento| %>
      <tr>
        <td><%= estacionamiento.fe_inicio %></td>
        <td><%= estacionamiento.fe_final %></td>
        <td><%= estacionamiento.monto_pagar %></td>
        <td><%= estacionamiento.mensaje %></td>
        <td><%= estacionamiento.status %></td>
        <td><%= estacionamiento.user %></td>
        <td><%= link_to 'Show', estacionamiento %></td>
        <td><%= link_to 'Edit', edit_estacionamiento_path(estacionamiento) %></td>
        <td><%= link_to 'Destroy', estacionamiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estacionamiento', new_estacionamiento_path %>
